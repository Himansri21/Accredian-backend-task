// This is your Prisma schema file,
datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model item {
  id          Int     @id @default(autoincrement())
  name        String
  email       String
  referralCode String
  refereeName String
  phoneNo     String
}

